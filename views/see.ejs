                                <th class="column2" style="text-align-last: center">TITLE</th>
                                <th class="column3" style="text-align-last: center">GRAPH</th>
                                <th class="column4" style="text-align-last: center">TIME LOAD</th>
                            </tr>
                        </thead>
                        <tbody>
                            <form action="/see" method="get">
                                <%data.forEach((dl,index) => {  %>
                                    <%let id = 'vl' + index%>
                                        <%let idtime = 'v' + index%>
                                            <tr>
                                                <td class="column1">
                                                    <%= dl.getUrl%>
                                                        <a href="/del/<%= dl._id%>/<%= dl.getUrl%>" class="fa fa-trash" aria-hidden="true"></a>
                                                </td>
                                                <td class="column2">
                                                    <%= dl.title%>
                                                </td>
                                                <td class="column3">
                                                    <p id=<%=id %>></p>
                                                </td>
                                                <td class="column4">
                                                    <p id=<%=idtime %>> 0ms</p>
                                                </td>

                                            </tr>
                                            <%})%>
                            </form>
                        </tbody>
                    </table>
                </div>
                <p style="text-align: center ; margin-top: 10px">Another account? <a href="/logout">logout</a></p>
                <p style="text-align: center ; margin-top: 10px">Reports? <a href="/reports">reports</a></p>
