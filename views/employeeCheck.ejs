    <link rel="stylesheet" type="text/css" href="css/util.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <style>
        body {
            background: linear-gradient(45deg, #49a09d, #5f2c82);
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
      <script>
         $(document).ready(function() {
             setInterval(checked, 60000);
            function checked() {
                $.ajax('/employeeOnline', {
                    dataType: 'script', 
                    timeout: 10000, 
                    success: function(data, status, xhr) {
                            let dt = JSON.parse(data);
                            let lengthDt = dt.length;
                            document.getElementById('p').value = lengthDt;
                            dt.map((item,index)=>{
                                document.getElementById("vl" + index).innerHTML = item;
                            })
                           
                        }
                    ,
                    error: function(jqXhr, textStatus, errorMessage) {
                        console.log(errorMessage); 
                    }
            });        
            }
           checked()
        });
      </script>
  </head>
  <body>
    <p id='p'></p>
    <div class="limiter">
        <div class="container-table100">
            <div class="wrap-table100">
                <div class="table100">
                    
                    <table>
                        <thead>
                            <tr class="table100-head">
                                <th class="column1">NAME EMPLOYEES</th>
                                <th class="column2">Order ID</th>
                                <th class="column3">Name</th>
                                <th class="column4">Price</th>
                            </tr>
                        </thead>
                        <tbody>
                        
                        <% for(let i =0 ; i< data-30 ;i++){ %>
                            <%let id = 'vl' + i%>
                            <tr>
                                <td  style='color:red'  id=<%=id %> ></td> 
                            </tr>
                            <%}%>
                        </tbody>
                    </table>
                </div>
            <p style="text-align: center ; margin-top: 10px">Another account? <a href="/logout">logout</a></p>
                <p style="text-align: center ; margin-top: 10px">Reports? <a href="/reports">reports</a>See: <a href="/see">see</a></p>
            </div>
        </div>
    </div>
    
    <!--===============================================================================================-->
    <script src="vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <!--===============================================================================================-->
    <script src="vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="js/main.js"></script>
  </body>
</html>